#!/bin/bash
#chmod +x run.sh

gcc client.c  `pkg-config --cflags --libs glib-2.0` -lm -lpthread -o cli
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' &
./cli <<< 'reader t11.pdf' ' 

wait

echo 'Script run'
exit
