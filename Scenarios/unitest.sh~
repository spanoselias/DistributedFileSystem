#!/bin/bash
#chmod +x run.sh

src=$1

gcc client.c  `pkg-config --cflags --libs glib-2.0` -lm -lpthread -o cli

 

START=$(date +%s);

 ./cli << 'EOF'
loggin spanos2 
read s2.mp3 
exit
EOF

END=$(date +%s);
echo $((END-START)) | awk '{print int($1/60)":"int($1%60)}' >> senario1.txt   


wait

echo 'Script run'
exit
